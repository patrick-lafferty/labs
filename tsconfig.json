{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "pretty": true,
        "strict": true
    },
    "include": [
        "./src/**/*"
    ]
}